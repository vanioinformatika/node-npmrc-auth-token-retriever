{
    "extends": "tslint:latest",
    "rules": {
        "no-consecutive-blank-lines": false,
        "member-ordering": false,
        "interface-name": false,
        "object-literal-sort-keys": false,
        "newline-before-return": true,
        "indent": [
            true,
            "spaces",
            4
        ],
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "never",
                    "typeLiterals": "never"
                }
            }
        ]
    }
}
